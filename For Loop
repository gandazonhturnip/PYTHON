angka2_list = [0, 1, 2, 3, 4]
print(angka2_list)


for i in angka2_list:
    print(f"i sekarang adalah = {i}")

print("end \n")

# ini dengan range
angka2_range = range(12)
print(angka2_range)

for x in angka2_range:
    print(f"a sekarang adalah = {x}")

print("end\n")

# menggunakan string

data_str = "Pagarbatu"
for y in data_str:
    print(y)

print("end")
